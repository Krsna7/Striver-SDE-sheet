class Solution
{
public:

    vector<int> ans;
    void solve(int index, vector<int>&arr, int sum)
    {
        if(index==arr.size())
        {
            ans.push_back(sum);
            return;
        }
        solve(index+1,arr,sum+arr[index]);
        solve(index+1,arr,sum);
    }
    
    vector<int> subsetSums(vector<int> arr, int N)
    {
       solve(0,arr,0);
       return ans;
    }
};
